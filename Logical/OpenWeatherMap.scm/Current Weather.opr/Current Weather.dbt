   <resourceConfig autoMapColumns="true" inputData="SELECT&#10;FROM_UNIXTIME(TRUNCATE(UNIX_TIMESTAMP(NOW())/900,0)*900, '%Y-%m-%dT%T') as last_update,&#10;cs.query,&#10;c.id as city_id,&#10;c.city as city_name,&#10;c.country,&#10;TRIM(TRAILING '.' FROM TRIM(TRAILING '0' FROM CONVERT(CAST(longitude AS DECIMAL(20,2)),CHAR(20)))) as longitude,&#10;TRIM(TRAILING '.' FROM TRIM(TRAILING '0' FROM CONVERT(CAST(c.latitude AS DECIMAL(20,2)),CHAR(20)))) as latitude,&#10;ds.apikey,&#10;w.units,&#10;w.lang,&#10;CONCAT(DATE_FORMAT(CURRENT_DATE(),'%Y-%m-%d'),'T',TIME_FORMAT(w.sun_rise,'%T')) as sun_rise,&#10;UNIX_TIMESTAMP(CONCAT(DATE_FORMAT(CURRENT_DATE(),'%Y-%m-%d'),'T',TIME_FORMAT(w.sun_rise,'%T'))) as sun_rise_epoch,&#10;CONCAT(DATE_FORMAT(CURRENT_DATE(),'%Y-%m-%d'),'T',TIME_FORMAT(w.sun_set,'%T')) as sun_set,&#10;UNIX_TIMESTAMP(CONCAT(DATE_FORMAT(CURRENT_DATE(),'%Y-%m-%d'),'T',TIME_FORMAT(w.sun_set,'%T'))) as sun_set_epoch,&#10;w.temp,&#10;w.temp_min,&#10;w.temp_max,&#10;w.temp_unit,&#10;w.humidity,&#10;w.pressure,&#10;w.wind_speed,&#10;w.wind_speed_name,&#10;w.wind_gusts,&#10;w.wind_direction,&#10;w.wind_direction_code,&#10;w.wind_direction_name,&#10;w.clouds,&#10;w.clouds_name,&#10;w.visibility,&#10;w.precipitation,&#10;w.precipitation_mode,&#10;w.precipitation_unit,&#10;w.weather_number,&#10;w.weather,&#10;w.weather_icon,&#10;w.humidity_unit,&#10;w.pressure_unit&#10;FROM data_set AS ds&#10;JOIN weather AS w ON ds.id=w.data_set_id&#10;JOIN city AS c ON c.id=w.city_id&#10;JOIN city_search AS cs ON cs.city_id=c.id&#10;" isQueryBased="true" loop="false" treatEmptyStringAsNull="false" treatTextAsEmpty="false" treatTextAsEmptyValue="" treatTextAsNull="false" treatTextAsNullValue="">
